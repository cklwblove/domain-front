{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "lib": ["es2015", "es2016", "es2017", "dom"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "skipDefaultLibCheck": true
  }
}
