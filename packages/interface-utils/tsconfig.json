{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "strict": false,
    "strictNullChecks": false,
    "typeRoots": ["src/types", "node_modules/@types"]
  },
  "include": ["src", "tsup.config.ts"],
  "typedocOptions": {
    "name": "interface utils",
    "out": "docs",
